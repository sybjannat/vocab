{
    "version": 2,
    "builds": [
        {
            "src": "api/server.py",
            "use": "@vercel/python"
        },
        {
            "src": "static/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/api/server.py"
        },
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/api/server.py"
        }
    ],
    "env": {
        "PYTHON_VERSION": "3.9"
    }
}